<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="app_core.twig.extension.output.class">App\CoreBundle\Twig\OutputExtension</parameter>
    </parameters>

    <services>
        <service id="app_core.twig.extension.output" class="%app_core.twig.extension.output.class%">
            <tag name="twig.extension" />
        </service>
    </services>
</container>
